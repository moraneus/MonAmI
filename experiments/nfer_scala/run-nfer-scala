#!/bin/bash

echo "=== RESULT ===" > result-nfer-scala

echo "########## spec 1 ##########" >> result-nfer-scala

(/usr/bin/time ./nfer spec1.nfer trace_1000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec1.nfer trace_2000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec1.nfer trace_4000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec1.nfer trace_8000.json false false) >> result-nfer-scala 2>&1 
(/usr/bin/time ./nfer spec1.nfer trace_16000.json false false) >> result-nfer-scala 2>&1


echo "########## spec 2 ##########" >> result-nfer-scala

(/usr/bin/time ./nfer spec2.nfer trace_1000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec2.nfer trace_2000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec2.nfer trace_4000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec2.nfer trace_8000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec2.nfer trace_16000.json false false) >> result-nfer-scala 2>&1

echo "########## spec 3 ########## - minimality" >> result-nfer-scala

(/usr/bin/time ./nfer spec3.nfer trace_1000.json true false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec3.nfer trace_2000.json true false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec3.nfer trace_4000.json true false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec3.nfer trace_8000.json true false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec3.nfer trace_16000.json true false) >> result-nfer-scala 2>&1

echo "########## spec 4 ##########" >> result-nfer-scala

(/usr/bin/time ./nfer spec4.nfer trace_1000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec4.nfer trace_2000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec4.nfer trace_4000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec4.nfer trace_8000.json false false) >> result-nfer-scala 2>&1
(/usr/bin/time ./nfer spec4.nfer trace_16000.json false false) >> result-nfer-scala 2>&1

