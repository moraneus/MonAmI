#!/bin/bash

echo "=== RESULT ===" > result-monami

echo "########### spec 1 ###########" >> result-monami
echo "--------- 1000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_1000.json) >> result-monami 2>&1
echo "--------- 2000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_2000.json) >> result-monami 2>&1
echo "--------- 4000 ----------" >> result-monami
(/usr/bin/time  python3.9 run_monami_experiments.py spec1.monami trace_4000.json) >> result-monami 2>&1
echo "--------- 8000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_8000.json) >> result-monami 2>&1
echo "--------- 16000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_16000.json) >> result-monami 2>&1


