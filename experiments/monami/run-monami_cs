#!/bin/bash

# export PYTHONPATH=$PYTHONPATH:`pwd`

echo "=== RESULT ===" > result-monami

echo "########### spec 1 ###########" >> result-monami
echo "--------- 1000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_1000_property_1.json 0) >> result-monami 2>&1
echo "--------- 2000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_2000_property_1.json 0) >> result-monami 2>&1
echo "--------- 4000 ----------" >> result-monami
(/usr/bin/time  python3.9 run_monami_experiments.py spec1.monami trace_4000_property_1.json 0) >> result-monami 2>&1
echo "--------- 8000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_8000_property_1.json 0) >> result-monami 2>&1
echo "--------- 16000 ----------" >> result-monami
(/usr/bin/time python3.9 run_monami_experiments.py spec1.monami trace_16000_property_1.json 0) >> result-monami 2>&1


