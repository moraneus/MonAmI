#!/bin/bash

echo "=== RESULT ===" > result-nfer-c

echo "########## spec 1 ##########" >> result-nfer-c

(/usr/bin/time python3.9 run-nfer-c.py spec1.nferc trace_1000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec1.nferc trace_2000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec1.nferc trace_4000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec1.nferc trace_8000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec1.nferc trace_16000.csv false false) >> result-nfer-c 2>&1

echo "########## spec 2 ##########" >> result-nfer-c

(/usr/bin/time python3.9 run-nfer-c.py spec2.nferc trace_1000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec2.nferc trace_2000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec2.nferc trace_4000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec2.nferc trace_8000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec2.nferc trace_16000.csv false false) >> result-nfer-c 2>&1

echo "########## spec 3 ########## - minimality" >> result-nfer-c

(/usr/bin/time python3.9 run-nfer-c.py spec3.nferc trace_1000.csv true false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec3.nferc trace_2000.csv true false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec3.nferc trace_4000.csv true false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec3.nferc trace_8000.csv true false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec3.nferc trace_16000.csv true false) >> result-nfer-c 2>&1

echo "########## spec 4 ##########" >> result-nfer-c

(/usr/bin/time python3.9 run-nfer-c.py spec4.nferc trace_1000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec4.nferc trace_2000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec4.nferc trace_4000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec4.nferc trace_8000.csv false false) >> result-nfer-c 2>&1
(/usr/bin/time python3.9 run-nfer-c.py spec4.nferc trace_16000.csv false false) >> result-nfer-c 2>&1
