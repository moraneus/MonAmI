#!/bin/bash

# export PYTHONPATH=$PYTHONPATH:`pwd`

echo "=== RESULT ===" > result-monami_cb

echo "########### spec 1 ###########" >> result-monami_cb
echo "--------- 1000 ----------" >> result-monami_cb
(gtime python3 run_monami_experiments.py spec1.monami trace_1000_property_1.json 1) >> result-monami_cb 2>&1
echo "--------- 2000 ----------" >> result-monami_cb
(gtime python3 run_monami_experiments.py spec1.monami trace_2000_property_1.json 1) >> result-monami_cb 2>&1
echo "--------- 4000 ----------" >> result-monami_cb
(gtime  python3 run_monami_experiments.py spec1.monami trace_4000_property_1.json 1) >> result-monami_cb 2>&1
echo "--------- 8000 ----------" >> result-monami_cb
(gtime python3 run_monami_experiments.py spec1.monami trace_8000_property_1.json 1) >> result-monami_cb 2>&1
echo "--------- 16000 ----------" >> result-monami_cb
(gtime python3 run_monami_experiments.py spec1.monami trace_16000_property_1.json 1) >> result-monami_cb 2>&1


